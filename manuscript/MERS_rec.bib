	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/29QSCRV6/Guindon and Gascuel - 2003 - A Simple, Fast, and Accurate Algorithm to Estimate.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/JPMSZQ8H/696.html:text/html},
	Issn = {1063-5157, 1076-836X},
