	Issn = {0022-538X, 1098-5514},
	Journal = {Journal of Virology},
	Language = {en},
	Month = apr,
	Number = {4},
	Pages = {1761--1764},
	Pmid = {8445709},
	Title = {A single amino acid in the {PB}2 gene of influenza A virus is a determinant of host range.},
