	Journal = {Antimicrobial Agents and Chemotherapy},
	Keywords = {Antiviral Agents, Base Sequence, Binding Sites, Codon, Drug Resistance, Microbial, {HIV}-1, {HIV} Reverse Transcriptase, Humans, Isoleucine, Lamivudine, Microbial Sensitivity Tests, Molecular Sequence Data, Mutation, Reverse Transcriptase Inhibitors, {RNA}-Directed {DNA} Polymerase, Stereoisomerism, Zalcitabine},
	Language = {eng},
