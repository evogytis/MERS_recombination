	File = {Full Text PDF:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/EME9TQMF/Baele et al. - 2012 - Improving the Accuracy of Demographic and Molecula.pdf:application/pdf;Snapshot:/Users/admin/Library/Application Support/Firefox/Profiles/tit72ymn.default/zotero/storage/4SKRJCCZ/2157.html:text/html},
	Issn = {0737-4038, 1537-1719},
	Journal = {Molecular Biology and Evolution},
	Keywords = {Bayes factors, Bayesian inference, {BEAST}, demographic models, marginal likelihood, model comparison, molecular clock, path sampling, Phylogeny, stepping-stone sampling},
	Language = {en},
